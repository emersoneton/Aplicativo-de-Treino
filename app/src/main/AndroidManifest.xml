<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apptreino">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Busca_Treino_Dieta"
            android:label="@string/title_activity_busca__treino__dieta"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Cadastro_Treino_Dieta"
            android:label="@string/title_activity_cadastro__treino__dieta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListViewAlunos"
            android:label="@string/title_activity_list_view_alunos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Aluno"
            android:label="@string/title_activity_aluno"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Professor"
            android:label="@string/title_activity_professor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>